<div role="tabpanel" class="tab-pane fade" id="best">
  <div class="ss_featured_products">
    <div class="owl-carousel owl-theme">
      <% @count_job_full_time.times do |n| %>
        <div class="item" data-hash=<%= n %> >
          <% job_start = n.to_i * 10 %>
          <% job_end = job_start.to_i + 9 %>
          <% @job_full_time.each_with_index do |job, index| %>
            <% if job_start <= index && index <= job_end %>
              <div class="jp_job_post_main_wrapper_cont">
                <div class="jp_job_post_main_wrapper">
                  <div class="row">
                    <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
                      <div class="jp_job_post_side_img">
                        <%= image_tag job.company.avatar.url, title: "post_img" %>
                      </div>
                      <div class="jp_job_post_right_cont">
                        <h4><%= job.title %></h4>
                        <p><%= job.company.name %></p>
                        <ul>
                          <li><i class="fas fa-money-bill-wave"></i>&nbsp; <%= number_with_delimiter job.salary %>$</li>
                          <li><i class="fa fa-map-marker"></i>&nbsp; <%= job.address %></li>
                        </ul>
                      </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                      <div class="jp_job_post_right_btn_wrapper">
                        <ul>
                          <li><a href="#"><i class="fa fa-heart-o"></i></a></li>
                          <li><a href="#"><%= checked_type_job job %></a></li>
                          <li><a href="#">Apply</a></li>
                        </ul>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="jp_job_post_keyword_wrapper">
                  <ul>
                    <li><i class="fa fa-tags"></i>Keywords :</li>
                    <li><a href="#">ui designer,</a></li>
                    <li><a href="#">developer,</a></li>
                    <li><a href="#">senior</a></li>
                    <li><a href="#">it company,</a></li>
                    <li><a href="#">design,</a></li>
                    <li><a href="#">call center</a></li>
                  </ul>
                </div>
              </div>
            <% end %>
          <% end %>
        </div>
      <% end %>
    </div>
  </div>
  <div class="video_nav_img_wrapper">
    <div class="video_nav_img">
      <ul>
        <li><a class="button secondary url owl_nav" href="#zero">1</a></li>
        <li><a class="button secondary url owl_nav" href="#one">2</a></li>
        <li><a class="button secondary url owl_nav active" href="#two">3</a></li>
      </ul>
    </div>
  </div>
</div>
